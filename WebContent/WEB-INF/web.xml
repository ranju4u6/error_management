	<servlet>
		<servlet-name>webappln</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>WEB-INF/em-servlet.xml</param-value>
		</init-param>

		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>webappln</servlet-name>
		<url-pattern>*.htm</url-pattern>
		<url-pattern>/j_spring_security_chk</url-pattern>
	</servlet-mapping>




</web-app>